<!--Partial for Monthly budget/remaining balance card in the overview-->
<div class='card'>
  <div class='card-body'>
    {{#each budgets as |budget|}}
      <h5 class='card-title'>Monthly Budget</h5>
      <p class='card-text'>${{budget.total_income}}</p>
      <h5 class='card-title'>Current Balance</h5>
      <p class='card-text'>${{budget.total_remain}}</p>
    {{/each}}
  </div>
</div>