<!--Partial of the saving goal card in the overview/homepage -->
<div class='card saving-card'>
  <div class='card-body'>
    {{#each budgets as |budget|}}
      <h5 class='card-title'>Savings Goal:</h5>
      <p class='card-text'>{{savings_goal_name}}</p>
      <p class='card-text'>${{savings_amount}}</p>
    {{/each}}
  </div>
</div>